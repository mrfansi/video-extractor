# API Settings
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4
API_PREFIX=/api

# Cloudflare R2 Settings
# Note: Cloudflare R2 uses 'auto' region, which is configured in the code
R2_ENDPOINT_URL=https://xxxxxxxxxxxx.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=your_access_key_id
R2_SECRET_ACCESS_KEY=your_secret_access_key
R2_BUCKET_NAME=your_bucket_name
R2_PUBLIC_URL=https://pub-xxxxxxxxxxxxxxxx.r2.dev

# Processing Settings
TEMP_DIR=/tmp/video-extractor
MAX_WORKERS=4
MAX_UPLOAD_SIZE_MB=500
